@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod: <http://moddevices.com/ns/mod#>.
@prefix modgui: <http://moddevices.com/ns/modgui#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://moddevices.com/plugins/mod-devel/mod-cv-to-parameter>
a lv2:Plugin, mod:ControlVoltagePlugin;

doap:name "CV to Parameter";
doap:license "GPL v2+";
doap:developer [
    foaf:name "Bram Giesen";
    foaf:homepage <>;
    foaf:mbox <mailto:>;
    ];

doap:maintainer [
    foaf:name "MOD";
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:bram@moddevices.com>;
    ];

    lv2:minorVersion 1;
    lv2:microVersion 0;

rdfs:comment """
""";

lv2:port
[
    a lv2:InputPort, lv2:CVPort;
    lv2:index 0;
    lv2:symbol "CVin";
    lv2:name "CV Input";
],
[
    a lv2:OutputPort, lv2:CVPort;
    lv2:index 1;
    lv2:minimum 0.0 ;
    lv2:maximum 10.0 ;
    lv2:symbol "CVout";
    lv2:name "CV Output";
]
,
[
    a lv2:InputPort ,
    lv2:ControlPort ;
    lv2:index 2 ;
    lv2:symbol "ParameterValue" ;
    lv2:name "Parameter Value" ;
    lv2:default 0.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 100.0 ;
    units:unit units:pc ;
]
,
[
    a lv2:InputPort ,
    lv2:ControlPort ;
    lv2:index 3 ;
    lv2:symbol "ModulationDepth" ;
    lv2:name "Modulation Depth" ;
    lv2:default 0.0 ;
    lv2:minimum -200.0 ;
    lv2:maximum 200.0 ;
    units:unit units:pc ;
]
,
[
    a lv2:InputPort ,
    lv2:ControlPort ;
    lv2:index 4 ;
    lv2:symbol "BYPASS" ;
    lv2:name "BYPASS" ;
    lv2:default 1.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 1.0 ;
    lv2:designation lv2:enabled;
    lv2:portProperty lv2:toggled;
]
.
