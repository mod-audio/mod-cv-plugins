@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix mod:  <http://moddevices.com/ns/mod#> .
@prefix opts: <http://lv2plug.in/ns/ext/options#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rsz:  <http://lv2plug.in/ns/ext/resize-port#> .
@prefix unit: <http://lv2plug.in/ns/extensions/units#> .

<http://moddevices.com/plugins/mod-devel/logic-operators>
a lv2:Plugin, mod:ControlVoltagePlugin ;

    lv2:extensionData <http://lv2plug.in/ns/ext/state#interface> ,
                      <http://kxstudio.sf.net/ns/lv2ext/programs#Interface> ;

    lv2:optionalFeature <http://lv2plug.in/ns/lv2core#hardRTCapable> ,
                        <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ;

    lv2:requiredFeature <http://lv2plug.in/ns/ext/options#options> ,
                        <http://lv2plug.in/ns/ext/urid#map> ;

    opts:supportedOption <http://lv2plug.in/ns/ext/buf-size#nominalBlockLength> ,
                         <http://lv2plug.in/ns/ext/buf-size#maxBlockLength> ,
                         <http://lv2plug.in/ns/ext/parameters#sampleRate> ;

    lv2:port [
        a lv2:InputPort, lv2:CVPort ;
        lv2:index 0 ;
        lv2:symbol "CV_in_1" ;
        lv2:name "CV_in_1" ;
    ] ,
    [
        a lv2:InputPort, lv2:CVPort ;
        lv2:index 1 ;
        lv2:symbol "CV_in_2" ;
        lv2:name "CV_in_2" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:CVPort ;
        lv2:index 2 ;
        lv2:symbol "CV_out" ;
        lv2:name "CV_out" ;
        lv2:minimum 0.0 ;
        lv2:maximum 5.0 ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 3 ;
        lv2:name """Logic Operator""" ;
        lv2:symbol "LogicOperator" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 6.000000 ;
        lv2:scalePoint [ rdfs:label "AND"; rdf:value 0; ] ;
        lv2:scalePoint [ rdfs:label "NAND"; rdf:value 1; ] ;
        lv2:scalePoint [ rdfs:label "INV"; rdf:value 2; ] ;
        lv2:scalePoint [ rdfs:label "OR"; rdf:value 3; ] ;
        lv2:scalePoint [ rdfs:label "NOR"; rdf:value 4; ] ;
        lv2:scalePoint [ rdfs:label "XOR"; rdf:value 5; ] ;
        lv2:scalePoint [ rdfs:label "XNOR"; rdf:value 6; ] ;
        lv2:portProperty lv2:enumeration;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name """Switch Point""" ;
        lv2:symbol "SwitchPoint" ;
        lv2:default 5.0;
        lv2:minimum -10.0;
        lv2:maximum 10.0;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name """Hysteresis""" ;
        lv2:symbol "Hysteresis" ;
        lv2:default 0.0;
        lv2:minimum 0.0;
        lv2:maximum 5.0;
    ] ;

    rdfs:comment """
Logic operators
""" ;

    mod:brand "MOD" ;
    mod:label "Logic Operators" ;

    doap:name """Logic Operators""" ;
    doap:license """Custom""" ;

    doap:maintainer [
        foaf:name """MOD""" ;
        foaf:homepage <http://moddevices.com> ;
    ] ;

    lv2:microVersion 0 ;
    lv2:minorVersion 2 .

