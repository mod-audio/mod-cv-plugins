@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix modgui: <http://moddevices.com/ns/modgui#>.
@prefix mod: <http://moddevices.com/ns/mod#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .

<http://moddevices.com/plugins/mod-devel/mod-cv-round>
a lv2:Plugin, mod:ControlVoltagePlugin;

doap:name "CV Round";
doap:license "GPL v2+";
doap:developer [
    foaf:name "Bram Giesen";
    foaf:homepage <>;
    foaf:mbox <mailto:bram@moddevices.com>;
    ];

doap:maintainer [
    foaf:name "MOD";
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:bram@moddevices.com>;
    ];

    lv2:minorVersion 1;
    lv2:microVersion 0;

rdfs:comment """
Plugin that can either return the integer value of the incoming signal or the decimal value only.
""";

lv2:port
[
    a lv2:InputPort, lv2:CVPort;
    lv2:index 0;
    lv2:symbol "CVin";
    lv2:name "CV Input";
],
[
    a lv2:OutputPort, lv2:CVPort;
    lv2:index 1;
    lv2:symbol "CVout1";
    lv2:name "CV Output";
],
[
    a lv2:InputPort ,
    lv2:ControlPort ;
    lv2:index 2 ;
    lv2:symbol "Mode" ;
    lv2:name "Mode";
    lv2:default 0 ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:scalePoint [ rdfs:label "Return integer value";                   rdf:value 0; ] ;
    lv2:scalePoint [ rdfs:label "Return decimal value";                 rdf:value 1; ] ;
    lv2:portProperty lv2:enumeration;
],
[
     a lv2:InputPort ,
     lv2:ControlPort ;
     lv2:index 3 ;
     lv2:symbol "PluginEnabled" ;
     lv2:name "PluginEnabled" ;
     lv2:default 1.0 ;
     lv2:minimum 0.0 ;
     lv2:maximum 1.0 ;
     lv2:designation lv2:enabled;
     lv2:portProperty lv2:toggled;
]
.
